<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="WebviewName" value="WEBVIEW_com.fourth.marketplace" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  </appSettings>
  <fourth driverType="Android" serverUrl="http://localhost:4548/wd/hub/" driverDirectory="" xdt:Transform="Replace">
    <reporting keepScreenshot="false" takeScreenshotOnError="false" />
	<timeout driverCommands="00:03:00"/>
    <capabilities>
      <add key="platformVersion" value="5.0.2"/>
      <add key="clearSystemFiles" value="true"/>
      <add key="deviceName" value="Galaxy Tab S"/>
      <add key="appPackage" value="com.fourth.qa"/>
      <add key="appActivity" value="com.salesforce.androidsdk.phonegap.ui.SalesforceDroidGapActivity"/>
      <add key="recreateChromeDriverSessions" value="true"/>
      <add key="autoGrantPermissions" value="true"/>
      <add key="chromeDriverPort" value="9501"/>
    </capabilities>
  </fourth>
</configuration>