<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <fourth driverType="InternetExplorer" serverUrl="" driverDirectory="" xdt:Transform="Replace">
    <reporting keepScreenshot="false" takeScreenshotOnError="false" />
    <timeout driverCommands="00:03:00" />
    <capabilities>
      <add key="nativeEvents" value="false" />
      <add key="unexpectedAlertBehaviour" value="accept" />
      <add key="ignoreProtectedModeSettings" value="true" />
      <add key="disable-popup-blocking" value="true" />
      <add key="enablePersistentHover" value="true" />
      <add key="ignoreZoomSetting" value="true" />
    </capabilities>
  </fourth>
</configuration>