<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="WebviewName" value="WEBVIEW_com.fourth.marketplace" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  </appSettings>
  <fourth driverType="Android" serverUrl="https://helpless-snake-45.localtunnel.me/wd/hub/" xdt:Transform="Replace">
    <timeout driverCommands="00:02:00"/>
    <capabilities>
      <add key="platformVersion" value="7.0"/>
      <add key="clearSystemFiles" value="true"/>
      <add key="deviceName" value="Galaxy S8+"/>
      <add key="appPackage" value="com.fourth.marketplace.qa"/>
      <add key="appActivity" value="com.salesforce.androidsdk.phonegap.ui.SalesforceDroidGapActivity"/>
      <add key="recreateChromeDriverSessions" value="true"/>
      <add key="autoGrantPermissions" value="true"/>
    </capabilities>
  </fourth>
</configuration>